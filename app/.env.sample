# Network Troubleshooting Assistant Configuration
# Make a copy of this file named ".env" and customize for your environment

# Document storage settings
DOCS_DIR=./network_docs
DB_DIR=./chroma_db

# Chunking and search settings
CHUNK_SIZE=512
CHUNK_OVERLAP=50
SEARCH_RESULTS=5

# Ollama settings
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=network-assistant

# Application settings
PORT=5000
DEBUG_MODE=False